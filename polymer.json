{
  "entrypoint": "index.html",
  "shell": "src/polyn-app.html",
  "fragments": [
    "src/home-view.html",
    "src/requests-view.html",
    "src/messages-view.html",
    "src/profile-view.html",
    "src/chat-view.html",
    "src/business-view.html",
    "src/item-view.html",
    "src/backendless-view.html",
    "src/pages-view.html",
    "src/animation-view.html",
    "src/error404-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "bundle": false,
    "addServiceWorker": true,
    "js": {"minify": false},
    "css": {"minify": false},
    "html": {"minify": false}
  }]
}
