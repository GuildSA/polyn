{
  "entrypoint": "index.html",
  "shell": "src/polyn-app.html",
  "fragments": [
    "src/home-view.html",
    "src/messages-view.html",
    "src/profile-view.html",
    "src/chat-view.html",
    "src/business-view.html",
    "src/item-view.html",
    "src/ajax-view.html",
    "src/backendless-view.html",
    "src/pageanim-view.html",
    "src/basicanim-view.html",
    "src/chatstart-view.html",
    "src/error404-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "firebase-messaging-sw.js",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/backendless/src/backendless.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "bundle": false,
    "addServiceWorker": true,
    "addPushManifest": true,
    "insertPrefetchLinks": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}
